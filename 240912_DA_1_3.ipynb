      "source": [
        "s = ['computer', 'python', 'sbs', 'sookmyung']\n",
        "\n",
        "def vowel(w):\n",
        "  for alp in w:\n",
        "    if (alp=='a' or alp=='e' or alp=='i' or alp=='o' or alp=='u' ):\n",
        "       yield alp\n",
        "\n",
        "for word in s:\n",
        "  result = []\n",
        "  for v in vowel(word):\n",
        "    result.append(v)\n",
        "  print(result)\n"
      ]
    }
